FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    INLET
    {
        type            fixedValue;
        value           uniform 0;
    }
    OUTLET
    {
        type            zeroGradient;
    }
    TOP
    {
        type            zeroGradient;
    }
    BOT
    {
        type            zeroGradient;
    }
    FRONT-WALL
    {
        type            symmetry;
    }
    BACK-WALL
    {
        type            symmetry;
    }
    CYLINDER
    {
        type            zeroGradient;
    }

}

